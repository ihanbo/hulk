<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@null">

    <TextView
        android:id="@+id/tv_key"
        android:layout_width="@dimen/mc_dimen_85_dip"
        android:layout_height="match_parent"
        android:gravity="center"
        android:textColor="@color/mc_color_667187"
        android:textSize="@dimen/mc_dimen_12_dip" />

    <View
        android:layout_width="0.5dp"
        android:layout_height="match_parent"
        android:layout_marginLeft="@dimen/mc_dimen_90_dip"
        android:background="@color/mc_10_black" />

    <com.hans.demo.mc.McMcCellsRecyclerView
        android:id="@+id/rv"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="@dimen/mc_dimen_90_dip">

    </com.hans.demo.mc.McMcCellsRecyclerView>

    <TextView
        android:id="@+id/tv_same"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:textSize="@dimen/mc_dimen_12_dip"
        android:textColor="@color/mc_color_192132"
        android:gravity="center"
        android:layout_marginLeft="@dimen/mc_dimen_85_dip" />

</FrameLayout>